    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongoose": "^5.4.12",
    "mysql2": "^1.6.5",
    "pluralize": "^8.0.0",
    "sequelize": "^5.8.12",
    "winston": "^3.2.1"
